    <style>
        html, body {
            margin: 0;
            padding: 0;
            overflow-x: hidden;
            width: 100%;
        }

        * {
            box-sizing: border-box;
        }

        .container {
            max-width: 100%;
        }

        html {
            scroll-behavior: smooth;
        }

        .blob {
            position: absolute;
            filter: blur(60px);
            opacity: 0.2;
            z-index: 2;
            animation: float 10s ease-in-out infinite;
            pointer-events: none;
        }

        .blob1 {
            top: 20%;
            left: 10%;
            animation-delay: 0s;
        }

        .blob2 {
            top: 45%;
            left: 25%;
            animation-delay: -1s;
        }

        .blob3 {
            bottom: 10%;
            left: 20%;
            animation-delay: -2s;
        }

        @keyframes float {
            0%, 100% {
                transform: translate(0, 0) rotate(0deg);
            }
            33% {
                transform: translate(30px, -50px) rotate(15deg);
            }
            66% {
                transform: translate(-20px, 30px) rotate(-10deg);
            }
        }

        .hero-content {
            position: relative;
            z-index: 1;
        }

        /* Responsive Styles */
        @media (max-width: 768px) {
            .hero-content {
                flex-direction: column;
                align-items: center;
                text-align: center;
            }

            .hero-content .lg\:w-1\/2 {
                width: 100%;
            }

            .blob {
                display: none; /* Hide blobs on smaller screens */
            }

            .absolute.left-10 {
                left: 50%;
                transform: translateX(-50%);
                top: 20%;
            }
        }

        @media (max-width: 640px) {
            .text-5xl {
                font-size: 2.5rem; /* Adjust font size for smaller screens */
            }

            .text-xl {
                font-size: 1.25rem; /* Adjust font size for smaller screens */
            }

            .btn {
                width: 100%; /* Make buttons full width */
                margin-bottom: 1rem; /* Add spacing between buttons */
            }
        }

        /* Additional styles for cards */
        .card {
            transition: transform 0.3s;
        }

        .card:hover {
            transform: scale(1.05);
        }

        .alert-container {
      position: fixed;
      top: 20px;
      left: 50%;
      transform: translateX(-50%);
      z-index: 9999;
      pointer-events: none;
    }

    .alert-box {
      background-color: #4CAF50;
      color: white;
      padding: 16px 24px;
      border-radius: 8px;
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
      display: flex;
      align-items: center;
      animation: slideDown 0.3s ease-out, fadeOut 0.3s ease-in 2.7s;
    }

    .alert-content {
      display: flex;
      align-items: center;
      gap: 8px;
      font-size: 16px;
      font-weight: 500;
    }

    .check-icon {
      width: 20px;
      height: 20px;
    }

    @keyframes slideDown {
      from {
        transform: translateY(-100%);
        opacity: 0;
      }
      to {
        transform: translateY(0);
        opacity: 1;
      }
    }

    @keyframes fadeOut {
      from {
        opacity: 1;
      }
      to {
        opacity: 0;
      }
    }
    </style>
</head>

