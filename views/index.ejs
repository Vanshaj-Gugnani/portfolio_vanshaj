                        <p class="text-gray-600"><%= experience.company %></p>
                        <p class="text-sm text-gray-500"><%= experience.date %></p>
                    </div>

                    <!-- Circle Indicator -->
                    <div class="relative flex items-center justify-center" style="z-index: 1;">
                        <div class="h-8 w-8 rounded-full bg-blue-500 shadow-lg"></div>
                    </div>

                    <!-- Right Content -->
                    <div class="flex-1 md:pl-8 pt-4 md:pt-0 relative" style="z-index: 1;">
                        <div class="bg-white p-6 rounded-lg shadow-lg">
                            <p class="text-gray-600"><%= experience.description %></p>
                        </div>
                    </div>
                </div>
            <% }); %>
    </section>


    <!-- Contact Section -->
    <section id="contact" class="py-20 bg-white">
        <div class="container mx-auto px-4">
            <h2 class="text-4xl font-bold text-center mb-12 text-gray-800">Get In Touch</h2>
            <div class="max-w-lg mx-auto">
                <form id="contactForm" action="/send-message" method="POST">
                    <div class="form-control mb-4">
                        <label class="label">
                            <span class="label-text text-gray-600">Name</span>
                        </label>
                        <input type="text" name="name" placeholder="Your Name" class="input input-bordered w-full bg-white" required />
                    </div>
                    <div class="form-control mb-4">
                        <label class="label">
                            <span class="label-text text-gray-600">Email</span>
                        </label>
                        <input type="email" name="email" placeholder="your.email@example.com" class="input input-bordered w-full bg-white" required />
                    </div>
                    <div class="form-control mb-4">
                        <label class="label">
                            <span class="label-text text-gray-600">Message</span>
                        </label>
                        <textarea name="message" class="textarea textarea-bordered h-32 bg-white" placeholder="Your message" required></textarea>
                    </div>
                    <button type="submit" class="btn btn-primary w-full">Send Message</button>
                </form>
            </div>
        </div>
    </section>
    
    
    <!-- Footer -->
    <footer class="footer footer-center p-10 bg-gray-50 text-gray-600 rounded">
        <div class="grid grid-flow-col gap-4">
            <a href="#about" class="link link-hover">About</a> 
            <a href="#projects" class="link link-hover">Projects</a> 
            <a href="#contact" class="link link-hover">Contact</a>
        </div>
        <div>
            <p>Copyright Â© 2024 - All rights reserved</p>
        </div>
    </footer>
</body>
</html>